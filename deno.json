{
  "tasks": {
    "dev:console": "cd apps/gb-console && pnpm run dev",
    "dev:player": "cd apps/gb-player && deno task dev",
    "dev:player:compile": "cd apps/gb-player && deno task compile",
    "dev:counter": "deno run -A --unstable-kv --watch counter.ts"
  },
  "nodeModulesDir": "auto",
  "imports": {
    "@libsql/client": "npm:@libsql/client@^0.15.3",
    "@std/assert": "jsr:@std/assert@1"
  },
  "unstable": ["unsafe-proto"],
  "workspace": ["./apps/gb-player"]
}
